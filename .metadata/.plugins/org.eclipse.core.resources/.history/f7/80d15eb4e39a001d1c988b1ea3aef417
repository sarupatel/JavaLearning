package learningOOP;

public class StringArray {

	private String[] daysOfWeek;

	public StringArray(String... daysOfWeek) {
		this.daysOfWeek = daysOfWeek;
	}

	public String longestName() {
		int[] longestWeekDay;
		int i=0;
		for (String dayOfWeek : daysOfWeek) {
			longestWeekDay[i]=dayOWeek.length();
			i++;
		}
		return ;
	}

}
